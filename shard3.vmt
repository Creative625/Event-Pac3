LightmappedGeneric
{
	$basetexture "animated/shard3"
	$selfillum 1
	$envmap env_cubemap
	$envmaptint "[.1 .1 .1]"

		"Proxies"
		{
			 "AnimatedTexture"
			{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			 "animatedtextureframerate" 29.995
			}
		}
}